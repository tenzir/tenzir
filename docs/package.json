{
  "name": "docs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "cp -f ../CHANGELOG.md src/pages/changelog.md && docusaurus start",
    "build": "cp -f ../CHANGELOG.md src/pages/changelog.md && docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "rm -f src/pages/changelog.md && docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@docusaurus/core": "2.0.0-beta.20",
    "@docusaurus/plugin-sitemap": "^2.0.0-beta.20",
    "@docusaurus/preset-classic": "2.0.0-beta.20",
    "@mdx-js/react": "^2.1.1",
    "clsx": "^1.1.1",
    "docusaurus-plugin-sass": "^0.2.2",
    "mdx-mermaid": "^1.2.2",
    "mermaid": "^9.1.1",
    "prism-react-renderer": "^1.3.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sass": "^1.52.1"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "2.0.0-beta.20",
    "@tsconfig/docusaurus": "^1.0.5",
    "typescript": "^4.6.4"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
