type timestamp = time
type port = count

type zeek.conn_id = record{
  orig_h: ip,
  orig_p: port,
  resp_h: ip,
  resp_p: port
}

type zeek.radius = record{
  _path: string,
  _write_ts: time,
  ts: timestamp,
  uid: string #index=hash,
  id: zeek.conn_id,
  username: string #skip,
  result: string
}
