---
description: "Greet someone with a custom message"
args:
  - name: person
    type: string
    description: "Name of the person to greet"
  - name: age
    type: int
    description: "Age of the person"
options:
  - name: prefix
    type: string
    default: "Hello"
    description: "Greeting prefix"
  - name: excited
    type: bool
    default: false
    description: "Use exclamation mark"
---
from {
  greeting: $prefix,
  name: $person,
  age: $age,
  excited: $excited
}
write_json
save_stdout
