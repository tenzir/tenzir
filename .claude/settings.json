{
  "hooks": {
    "Setup": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "git submodule update --init --recursive"
          }
        ]
      }
    ]
  },
  "extraKnownMarketplaces": {
    "tenzir": {
      "source": {
        "source": "github",
        "repo": "tenzir/claude-plugins"
      }
    }
  },
  "enabledPlugins": {
    "cpp@tenzir": true,
    "docs@tenzir": true,
    "formatter@tenzir": true,
    "git@tenzir": true,
    "ocsf@tenzir": true,
    "plan@tenzir": true,
    "prose@tenzir": true,
    "python@tenzir": true,
    "ship@tenzir": true,
    "tql@tenzir": true
  }
}
