// Test from_s3 with role-only authentication.
// Uses the default credential chain (AWS_ACCESS_KEY_ID/AWS_SECRET_ACCESS_KEY
// from environment) to assume a role, then reads from S3.
from_s3 env("LOCALSTACK_S3_TEST_URI"), aws_iam={
  region: "us-east-1",
  assume_role: env("LOCALSTACK_ROLE_ARN"),
  external_id: env("LOCALSTACK_EXTERNAL_ID")
}
